 .timescale 0 0;
S_000002169af62e70 .scope module, "tb" "tb" 3 1;
 .timescale 0 0;
S_000002169af4b290 .scope task, "display" "display" 3 11, 3 11 0, S_000002169af62e70;
 .timescale 0 0;
TD_tb.display ;
    %fork t_1, S_000002169af4b5b0;
    %jmp t_0;
    .scope S_000002169af4b5b0;
t_1 ;
    %vpi_call/w 3 13 "$display", "[%0t] T_Task started", $time {0 0 0};
    %delay 100, 0;
    %vpi_call/w 3 15 "$display", "[%0t] T_Task ended", $time {0 0 0};
    %end;
    .scope S_000002169af4b290;
t_0 %join;
    %fork t_3, S_000002169af4b420;
    %jmp t_2;
    .scope S_000002169af4b420;
t_3 ;
    %delay 10, 0;
    %vpi_call/w 3 20 "$display", "[%0t] S_Task started", $time {0 0 0};
    %delay 20, 0;
    %vpi_call/w 3 22 "$display", "[%0t] S_Task ended", $time {0 0 0};
    %end;
    .scope S_000002169af4b290;
t_2 %join;
    %end;
S_000002169af4b420 .scope begin, "S_DISPLAY" "S_DISPLAY" 3 18, 3 18 0, S_000002169af4b290;
 .timescale 0 0;
S_000002169af4b5b0 .scope begin, "T_DISPLAY" "T_DISPLAY" 3 12, 3 12 0, S_000002169af4b290;
 .timescale 0 0;
    .scope S_000002169af62e70;
T_1 ;
    %fork TD_tb.display, S_000002169af4b290;
    %join;
    %end;
    .thread T_1;
    .scope S_000002169af62e70;
T_2 ;
    %delay 50, 0;
    %disable S_000002169af4b5b0;
    %end;
    .thread T_2;
