:ivl_version "12.0 (devel)" "(s20150603-1539-g2693dd32b)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "D:\iverilog\lib\ivl\system.vpi";
:vpi_module "D:\iverilog\lib\ivl\vhdl_sys.vpi";
:vpi_module "D:\iverilog\lib\ivl\vhdl_textio.vpi";
:vpi_module "D:\iverilog\lib\ivl\v2005_math.vpi";
:vpi_module "D:\iverilog\lib\ivl\va_math.vpi";
:vpi_module "D:\iverilog\lib\ivl\v2009.vpi";
S_000001678a09b9e0 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_000001678a09bb70 .scope module, "tb" "tb" 3 14;
 .timescale 0 0;
v000001678a098230_0 .var/str "firstname";
v000001678a1230e0_0 .var/str "lastname";
    .scope S_000001678a09bb70;
T_0 ;
    %pushi/str "Joey";
    %store/str v000001678a098230_0;
    %pushi/str "Tribbiani";
    %store/str v000001678a1230e0_0;
    %end;
    .thread T_0, $init;
    .scope S_000001678a09bb70;
T_1 ;
    %load/str v000001678a098230_0;
    %load/str v000001678a1230e0_0;
    %cmp/str;
    %flag_get/vec4 4;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %vpi_call/w 3 21 "$display", "firstname=%s is EQUAL to lastname=%s", v000001678a098230_0, v000001678a1230e0_0 {0 0 0};
T_1.0 ;
    %load/str v000001678a098230_0;
    %load/str v000001678a1230e0_0;
    %cmp/str;
    %flag_inv 4;
    %flag_get/vec4 4;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.2, 8;
    %vpi_call/w 3 24 "$display", "firstname=%s is NOT EQUAL to lastname=%s", v000001678a098230_0, v000001678a1230e0_0 {0 0 0};
T_1.2 ;
    %load/str v000001678a098230_0;
    %load/str v000001678a1230e0_0;
    %cmp/str;
    %flag_get/vec4 5;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.4, 8;
    %vpi_call/w 3 28 "$display", "firstname=%s is LESS THAN lastname=%s", v000001678a098230_0, v000001678a1230e0_0 {0 0 0};
T_1.4 ;
    %load/str v000001678a1230e0_0;
    %load/str v000001678a098230_0;
    %cmp/str;
    %flag_get/vec4 5;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.6, 8;
    %vpi_call/w 3 32 "$display", "firstname=%s is GREATER THAN lastname=%s", v000001678a098230_0, v000001678a1230e0_0 {0 0 0};
T_1.6 ;
    %load/str v000001678a098230_0;
    %concati/str " ";
    %load/str v000001678a1230e0_0;
    %concat/str;
    %vpi_call/w 3 35 "$display", "Full Name = %s", S<0,str> {0 0 1};
    %load/str v000001678a098230_0;
    %load/str v000001678a098230_0;
    %concat/str;
    %load/str v000001678a098230_0;
    %concat/str;
    %vpi_call/w 3 38 "$display", "%s", S<0,str> {0 0 1};
    %load/str v000001678a098230_0;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %substr/vec4 4, 8;
    %pop/str 1;
    %load/str v000001678a1230e0_0;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %substr/vec4 4, 8;
    %pop/str 1;
    %vpi_call/w 3 41 "$display", "firstname[2]=%s lastname[2]=%s", S<1,vec4,u8>, S<0,vec4,u8> {2 0 0};
    %end;
    .thread T_1;
