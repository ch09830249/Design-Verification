E_00000263444d5950 .event "event_a";
    .scope S_00000263444a2ce0;
T_0 ;
    %delay 20, 0;
    %event E_00000263444d5950;
    %vpi_call/w 3 9 "$display", "[%0t] Thread1: triggered event_a", $time {0 0 0};
    %end;
    .thread T_0;
    .scope S_00000263444a2ce0;
T_1 ;
    %vpi_call/w 3 14 "$display", "[%0t] Thread2: waiting for trigger ", $time {0 0 0};
