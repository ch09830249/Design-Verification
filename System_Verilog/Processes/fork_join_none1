S_0000028e98f72df0 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0000028e98f72f80 .scope module, "tb" "tb" 3 1;
 .timescale 0 0;
S_0000028e990cb920 .scope autotask, "print" "print" 3 16, 3 16 0, S_0000028e98f72f80;
 .timescale 0 0;
v0000028e990cb740_0 .var/2s "_time", 31 0;
v0000028e98febc40_0 .var/str "t_name";
TD_tb.print ;
    %load/vec4 v0000028e990cb740_0;
    %pad/s 64;
    %muli 1, 0, 64;
    %ix/vec4 4;
    %delayx 4;
    %vpi_call/w 3 17 "$display", "[%0t] %s", $time, v0000028e98febc40_0 {0 0 0};
    %end;
    .scope S_0000028e98f72f80;
T_1 ;
    %vpi_call/w 3 3 "$display", "[%0t] Main Thread: Fork join going to start", $time {0 0 0};
    %fork t_1, S_0000028e98f72f80;
    %fork t_2, S_0000028e98f72f80;
    %fork t_3, S_0000028e98f72f80;
    %join/detach 3;
    %jmp t_0;
t_1 ;
    %alloc S_0000028e990cb920;
    %pushi/vec4 20, 0, 32;
    %store/vec4 v0000028e990cb740_0, 0, 32;
    %pushi/str "Thread1";
    %store/str v0000028e98febc40_0;
    %fork TD_tb.print, S_0000028e990cb920;
    %join;
    %free S_0000028e990cb920;
    %end;
t_2 ;
    %alloc S_0000028e990cb920;
    %pushi/vec4 30, 0, 32;
    %store/vec4 v0000028e990cb740_0, 0, 32;
    %pushi/str "Thread2";
    %store/str v0000028e98febc40_0;
    %fork TD_tb.print, S_0000028e990cb920;
    %join;
    %free S_0000028e990cb920;
    %end;
t_3 ;
    %alloc S_0000028e990cb920;
    %pushi/vec4 10, 0, 32;
    %store/vec4 v0000028e990cb740_0, 0, 32;
    %pushi/str "Thread3";
    %store/str v0000028e98febc40_0;
    %fork TD_tb.print, S_0000028e990cb920;
    %join;
    %free S_0000028e990cb920;
    %end;
    .scope S_0000028e98f72f80;
t_0 ;
    %vpi_call/w 3 12 "$display", "[%0t] Main Thread: Fork join has finished", $time {0 0 0};
    %end;
    .thread T_1;
