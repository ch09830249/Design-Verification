#! /c/Source/iverilog-install/bin/vvp
:ivl_version "12.0 (devel)" "(s20150603-1539-g2693dd32b)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "D:\iverilog\lib\ivl\system.vpi";
:vpi_module "D:\iverilog\lib\ivl\vhdl_sys.vpi";
:vpi_module "D:\iverilog\lib\ivl\vhdl_textio.vpi";
:vpi_module "D:\iverilog\lib\ivl\v2005_math.vpi";
:vpi_module "D:\iverilog\lib\ivl\va_math.vpi";
:vpi_module "D:\iverilog\lib\ivl\v2009.vpi";
S_00000202ff302df0 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_00000202ff302f80 .scope module, "tb" "tb" 3 1;
 .timescale 0 0;
S_00000202ff47c450 .scope autotask, "print" "print" 3 20, 3 20 0, S_00000202ff302f80;
 .timescale 0 0;
v00000202ff47b3f0_0 .var/2s "_time", 31 0;
v00000202ff47eef0_0 .var/str "t_name";
TD_tb.print ;
    %load/vec4 v00000202ff47b3f0_0;
    %pad/s 64;
    %muli 1, 0, 64;
    %ix/vec4 4;
    %delayx 4;
    %vpi_call/w 3 21 "$display", "[%0t] %s", $time, v00000202ff47eef0_0 {0 0 0};
    %end;
    .scope S_00000202ff302f80;
T_1 ;
    %vpi_call/w 3 3 "$display", "[%0t] Main Thread: Fork join going to start", $time {0 0 0};
    %fork t_1, S_00000202ff302f80;
    %fork t_2, S_00000202ff302f80;
    %join;
    %join;
    %jmp t_0;
t_1 ;
    %fork t_4, S_00000202ff302f80;
    %fork t_5, S_00000202ff302f80;
    %join;
    %join;
    %jmp t_3;
t_4 ;
    %alloc S_00000202ff47c450;
    %pushi/vec4 20, 0, 32;
    %store/vec4 v00000202ff47b3f0_0, 0, 32;
    %pushi/str "Thread1_0";
    %store/str v00000202ff47eef0_0;
    %fork TD_tb.print, S_00000202ff47c450;
    %join;
    %free S_00000202ff47c450;
    %end;
t_5 ;
    %alloc S_00000202ff47c450;
    %pushi/vec4 30, 0, 32;
    %store/vec4 v00000202ff47b3f0_0, 0, 32;
    %pushi/str "Thread1_1";
    %store/str v00000202ff47eef0_0;
    %fork TD_tb.print, S_00000202ff47c450;
    %join;
    %free S_00000202ff47c450;
    %end;
    .scope S_00000202ff302f80;
t_3 ;
    %end;
t_2 ;
    %alloc S_00000202ff47c450;
    %pushi/vec4 10, 0, 32;
    %store/vec4 v00000202ff47b3f0_0, 0, 32;
    %pushi/str "Thread2";
    %store/str v00000202ff47eef0_0;
    %fork TD_tb.print, S_00000202ff47c450;
    %join;
    %free S_00000202ff47c450;
    %end;
    .scope S_00000202ff302f80;
t_0 ;
    %vpi_call/w 3 16 "$display", "[%0t] Main Thread: Fork join has finished", $time {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
