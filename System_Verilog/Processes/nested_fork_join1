S_000002816926a230 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_000002816926bbf0 .scope module, "tb" "tb" 3 1;
 .timescale 0 0;
    .scope S_000002816926bbf0;
T_0 ;
    %vpi_call/w 3 3 "$display", "[%0t] Main Thread: Fork join going to start", $time {0 0 0};
    %fork t_1, S_000002816926bbf0;
    %fork t_2, S_000002816926bbf0;
    %fork t_3, S_000002816926bbf0;
    %join;
    %join;
    %join;
    %jmp t_0;
t_1 ;
    %fork t_5, S_000002816926bbf0;
    %fork t_6, S_000002816926bbf0;
    %fork t_7, S_000002816926bbf0;
    %join;
    %join;
    %join;
    %jmp t_4;
t_5 ;
    %delay 50, 0;
    %vpi_call/w 3 8 "$display", "[%0t] Thread1_0 ...", $time {0 0 0};
    %end;
t_6 ;
    %delay 70, 0;
    %vpi_call/w 3 10 "$display", "[%0t] Thread1_1 ...", $time {0 0 0};
    %end;
t_7 ;
    %delay 10, 0;
    %vpi_call/w 3 13 "$display", "[%0t] Thread1_2 ...", $time {0 0 0};
    %delay 100, 0;
    %vpi_call/w 3 14 "$display", "[%0t] Thread1_2 finished", $time {0 0 0};
    %end;
    .scope S_000002816926bbf0;
t_4 ;
    %end;
t_2 ;
    %delay 5, 0;
    %vpi_call/w 3 20 "$display", "[%0t] Thread2 ...", $time {0 0 0};
    %delay 10, 0;
    %vpi_call/w 3 21 "$display", "[%0t] Thread2 finished", $time {0 0 0};
    %end;
t_3 ;
    %delay 20, 0;
    %vpi_call/w 3 25 "$display", "[%0t] Thread3 finished", $time {0 0 0};
    %end;
    .scope S_000002816926bbf0;
t_0 ;
    %vpi_call/w 3 27 "$display", "[%0t] Main Thread: Fork join has finished", $time {0 0 0};
    %end;
    .thread T_0;
