#! /c/Source/iverilog-install/bin/vvp
:ivl_version "12.0 (devel)" "(s20150603-1539-g2693dd32b)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "D:\iverilog\lib\ivl\system.vpi";
:vpi_module "D:\iverilog\lib\ivl\vhdl_sys.vpi";
:vpi_module "D:\iverilog\lib\ivl\vhdl_textio.vpi";
:vpi_module "D:\iverilog\lib\ivl\v2005_math.vpi";
:vpi_module "D:\iverilog\lib\ivl\va_math.vpi";
:vpi_module "D:\iverilog\lib\ivl\v2009.vpi";
S_000002316a547e30 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_000002316a547fc0 .scope module, "tb_top" "tb_top" 3 5;
 .timescale 0 0;
    .scope S_000002316a547fc0;
T_0 ;
    %fork t_1, S_000002316a547fc0;
    %fork t_2, S_000002316a547fc0;
    %fork t_3, S_000002316a547fc0;
    %join;
    %join/detach 2;
    %jmp t_0;
t_1 ;
    %delay 40, 0;
    %vpi_call/w 3 13 "$display", "[%0t ns] Show #40 $display statement", $time {0 0 0};
    %end;
t_2 ;
    %delay 20, 0;
    %vpi_call/w 3 17 "$display", "[%0t ns] Show #20 $display statement", $time {0 0 0};
    %delay 50, 0;
    %vpi_call/w 3 18 "$display", "[%0t ns] Show #50 $display statement", $time {0 0 0};
    %end;
t_3 ;
    %delay 60, 0;
    %vpi_call/w 3 22 "$display", "[%0t ns] TIMEOUT", $time {0 0 0};
    %end;
    .scope S_000002316a547fc0;
t_0 ;
    %vpi_call/w 3 26 "$display", "[%0t ns] Fork join is done, wait fork to end", $time {0 0 0};
    %wait/fork;
    %vpi_call/w 3 30 "$display", "[%0t ns] Fork join is over", $time {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
