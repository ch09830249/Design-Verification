S_0000026311cf2ce0 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0000026311cf2e70 .scope module, "tb" "tb" 3 1;
 .timescale 0 0;
S_0000026311cdc700 .scope autotask, "print" "print" 3 22, 3 22 0, S_0000026311cf2e70;
 .timescale 0 0;
v0000026311cdc250_0 .var/2s "_time", 31 0;
v0000026311cf3000_0 .var/str "t_name";
TD_tb.print ;
    %load/vec4 v0000026311cdc250_0;
    %pad/s 64;
    %muli 1, 0, 64;
    %ix/vec4 4;
    %delayx 4;
    %vpi_call/w 3 23 "$display", "[%0t] %s", $time, v0000026311cf3000_0 {0 0 0};
    %end;
    .scope S_0000026311cf2e70;
T_1 ;
    %vpi_call/w 3 3 "$display", "[%0t] Main Thread: Fork join going to start", $time {0 0 0};
    %fork t_1, S_0000026311cf2e70;
    %fork t_2, S_0000026311cf2e70;
    %join/detach 2;
    %jmp t_0;
t_1 ;
    %fork t_4, S_0000026311cf2e70;
    %fork t_5, S_0000026311cf2e70;
    %join/detach 2;
    %jmp t_3;
t_4 ;
    %alloc S_0000026311cdc700;
    %pushi/vec4 20, 0, 32;
    %store/vec4 v0000026311cdc250_0, 0, 32;
    %pushi/str "Thread1_0";
    %store/str v0000026311cf3000_0;
    %fork TD_tb.print, S_0000026311cdc700;
    %join;
    %free S_0000026311cdc700;
    %end;
t_5 ;
    %alloc S_0000026311cdc700;
    %pushi/vec4 30, 0, 32;
    %store/vec4 v0000026311cdc250_0, 0, 32;
    %pushi/str "Thread1_1";
    %store/str v0000026311cf3000_0;
    %fork TD_tb.print, S_0000026311cdc700;
    %join;
    %free S_0000026311cdc700;
    %end;
    .scope S_0000026311cf2e70;
t_3 ;
    %vpi_call/w 3 13 "$display", "[%0t] Nested fork has finished", $time {0 0 0};
    %end;
t_2 ;
    %alloc S_0000026311cdc700;
    %pushi/vec4 10, 0, 32;
    %store/vec4 v0000026311cdc250_0, 0, 32;
    %pushi/str "Thread2";
    %store/str v0000026311cf3000_0;
    %fork TD_tb.print, S_0000026311cdc700;
    %join;
    %free S_0000026311cdc700;
    %end;
    .scope S_0000026311cf2e70;
t_0 ;
    %vpi_call/w 3 18 "$display", "[%0t] Main Thread: Fork join has finished", $time {0 0 0};
    %end;
    .thread T_1;
