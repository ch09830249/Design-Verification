:vpi_module "D:\iverilog\lib\ivl\v2009.vpi";
S_000001e2b249ab00 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_000001e2b2362510 .scope module, "tb" "tb" 3 1;
 .timescale 0 0;
v000001e2b249ac90_0 .var/2u "clk", 0 0;
S_000001e2b23626a0 .scope begin, "$unm_blk_1" "$unm_blk_1" 3 6, 3 6 0, S_000001e2b2362510;
 .timescale 0 0;
v000001e2b2498c60_0 .var/2u "counter", 3 0;
E_000001e2b249b460 .event posedge, v000001e2b249ac90_0;
    .scope S_000001e2b2362510;
T_0 ;
    %delay 10, 0;
    %load/vec4 v000001e2b249ac90_0;
    %inv;
    %store/vec4 v000001e2b249ac90_0, 0, 1;
    %jmp T_0;
    .thread T_0;
    .scope S_000001e2b2362510;
T_1 ;
    %fork t_1, S_000001e2b23626a0;
    %jmp t_0;
    .scope S_000001e2b23626a0;
t_1 ;
    %vpi_call/w 3 9 "$display", "%0t: Original Counter = %0d, clk = %0d", $time, v000001e2b2498c60_0, v000001e2b249ac90_0 {0 0 0};
T_1.0 ;
    %load/vec4 v000001e2b2498c60_0;
    %pad/u 32;
    %cmpi/u 10, 0, 32;
    %jmp/0xz T_1.1, 5;
    %wait E_000001e2b249b460;
    ; show_stmt_assign_vector: Get l-value for compressed += operand
    %load/vec4 v000001e2b2498c60_0;
    %pushi/vec4 1, 0, 4;
    %add;
    %cast2;
    %store/vec4 v000001e2b2498c60_0, 0, 4;
    %vpi_call/w 3 13 "$display", "%0t: After ++, Counter = %0d", $time, v000001e2b2498c60_0 {0 0 0};
    %jmp T_1.0;
T_1.1 ;
    %vpi_call/w 3 15 "$display", "%0t: Final Counter = %0d", $time, v000001e2b2498c60_0 {0 0 0};
    %vpi_call/w 3 16 "$finish" {0 0 0};
    %end;
    .scope S_000001e2b2362510;
t_0 %join;
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
